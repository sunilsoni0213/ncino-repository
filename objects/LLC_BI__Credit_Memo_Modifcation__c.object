    <fields>
        <fullName>CSiDP__Modification_Description__c</fullName>
        <deprecated>false</deprecated>
        <description>Users manually populate this optional text field. This is a description of the specific modifications made to the existing loan agreement. This description appears in the Change In Terms Agreement for CSi DocPrep.</description>
        <externalId>false</externalId>
        <inlineHelpText>Populate this field to appear in the Change In Terms Agreement for CSi DocPrep</inlineHelpText>
        <label>Modification Description</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CSiDP__Modification_Document_Type__c</fullName>
        <deprecated>false</deprecated>
        <description>Users manually populate this optional text field. This field identifies types of documents the borrower must provide the lender in order to modify an existing loan. This information will determine which documents are included in a particular loan modification.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field identifies types of documents the borrower must provide the lender in a loan modification.</inlineHelpText>
        <label>Modification Document Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Returns the modified Commercial Promissory Note, and the accompanying collateral documents.</fullName>
                    <default>false</default>
                    <label>Returns the modified Commercial Promissory Note, and the accompanying collateral documents.</label>
                </value>
                <value>
                    <fullName>Returns only the modified Commercial Promissory Note; does not return the collateral documents.</fullName>
                    <default>false</default>
                    <label>Returns only the modified Commercial Promissory Note; does not return the collateral documents.</label>
                </value>
                <value>
                    <fullName>Returns the Change in Terms Agreement in lieu of the Commercial Promissory Note, and the accompanying collateral documents.</fullName>
                    <default>false</default>
                    <label>Returns the Change in Terms Agreement in lieu of the Commercial Promissory Note, and the accompanying collateral documents.</label>
                </value>
                <value>
                    <fullName>Returns only the Change in Terms Agreement in lieu of the Commercial Promissory Note; does not return the collateral documents.</fullName>
                    <default>false</default>
                    <label>Returns only the Change in Terms Agreement in lieu of the Commercial Promissory Note; does not return the collateral documents.</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
